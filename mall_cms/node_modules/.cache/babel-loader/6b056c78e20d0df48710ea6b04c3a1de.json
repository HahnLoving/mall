{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/web.dom-collections.for-each.js\";\nexport default {\n  data: function data() {\n    return {\n      show: false\n    };\n  },\n  computed: {\n    stageInfo: function stageInfo() {\n      return this.$store.getters.getStageInfo(this.$route.name);\n    },\n    menuTabs: function menuTabs() {\n      if (this.stageInfo.length < 2) {\n        return [];\n      }\n\n      var father = this.stageInfo[this.stageInfo.length - 2];\n\n      if (father.type === 'tab') {\n        var menus = [];\n        father.children.forEach(function (item) {\n          if (item.inNav) {\n            menus.push({\n              icon: item.icon || '',\n              title: item.title,\n              path: item.route\n            });\n          }\n        });\n        return menus;\n      }\n\n      return [];\n    }\n  }\n};","map":{"version":3,"sources":["/Users/zhahn/Desktop/未命名文件夹/lin-cms-vue/src/component/layout/menu-tab.vue"],"names":[],"mappings":";;;AAeA,eAAe;AACb,EAAA,IADa,kBACN;AACL,WAAO;AACL,MAAA,IAAI,EAAE;AADD,KAAP;AAGD,GALY;AAMb,EAAA,QAAQ,EAAE;AACR,IAAA,SADQ,uBACI;AACV,aAAO,KAAK,MAAL,CAAY,OAAZ,CAAoB,YAApB,CAAiC,KAAK,MAAL,CAAY,IAA7C,CAAP;AACD,KAHO;AAIR,IAAA,QAJQ,sBAIG;AACT,UAAI,KAAK,SAAL,CAAe,MAAf,GAAwB,CAA5B,EAA+B;AAC7B,eAAO,EAAP;AACF;;AACA,UAAM,MAAK,GAAI,KAAK,SAAL,CAAe,KAAK,SAAL,CAAe,MAAf,GAAwB,CAAvC,CAAf;;AACA,UAAI,MAAM,CAAC,IAAP,KAAgB,KAApB,EAA2B;AACzB,YAAM,KAAI,GAAI,EAAd;AACA,QAAA,MAAM,CAAC,QAAP,CAAgB,OAAhB,CAAwB,UAAA,IAAG,EAAK;AAC9B,cAAI,IAAI,CAAC,KAAT,EAAgB;AACd,YAAA,KAAK,CAAC,IAAN,CAAW;AACT,cAAA,IAAI,EAAE,IAAI,CAAC,IAAL,IAAa,EADV;AAET,cAAA,KAAK,EAAE,IAAI,CAAC,KAFH;AAGT,cAAA,IAAI,EAAE,IAAI,CAAC;AAHF,aAAX;AAKF;AACD,SARD;AASA,eAAO,KAAP;AACF;;AACA,aAAO,EAAP;AACD;AAvBO;AANG,CAAf","sourcesContent":["<template>\n  <div>\n    <div v-if=\"menuTabs.length || show\">\n      <ul class=\"menu-tab\">\n        <router-link :to=\"tab.path\" v-for=\"tab in menuTabs\" :key=\"tab.path\" ref=\"menuTabs\">\n          <li ref=\"tabList\" class=\"menu-li\">\n            <i :class=\"tab.icon\" /> <span class=\"title\">{{ $filters.filterTitle(tab.title) }}</span>\n          </li>\n        </router-link>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      show: false,\n    }\n  },\n  computed: {\n    stageInfo() {\n      return this.$store.getters.getStageInfo(this.$route.name)\n    },\n    menuTabs() {\n      if (this.stageInfo.length < 2) {\n        return []\n      }\n      const father = this.stageInfo[this.stageInfo.length - 2]\n      if (father.type === 'tab') {\n        const menus = []\n        father.children.forEach(item => {\n          if (item.inNav) {\n            menus.push({\n              icon: item.icon || '',\n              title: item.title,\n              path: item.route,\n            })\n          }\n        })\n        return menus\n      }\n      return []\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.router-link-active {\n  background: black;\n}\n\n.menu-tab {\n  width: 100%;\n  height: 38px;\n  line-height: 38px;\n  background: $reuse-tab-item-background;\n  font-size: 14px;\n  font-weight: 400;\n  color: rgba(140, 152, 174, 1);\n  display: flex;\n  flex-direction: row;\n  justify-content: flex-start;\n\n  .router-link-exact-active,\n  .router-link-active {\n    background: $app-main-background;\n    color: $theme;\n  }\n\n  .menu-li {\n    width: 120px;\n    height: 38px;\n    cursor: pointer;\n    display: flex;\n    justify-content: center;\n\n    .imgIcon {\n      width: 16px;\n      height: 16px;\n      margin: 0 auto;\n    }\n    .title {\n      margin-left: 5px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}