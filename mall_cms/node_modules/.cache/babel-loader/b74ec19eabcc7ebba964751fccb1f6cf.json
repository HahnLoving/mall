orm,\n      handleEdit,\n      confirmEdit,\n      handleClose,\n      handleDelete,\n      rowDoubleClick,\n      goToGroupEditPage,\n      dialogFormVisible,\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.container {\n  padding: 0 30px;\n\n  .title {\n    height: 59px;\n    line-height: 59px;\n    color: $parent-title-color;\n    font-size: 16px;\n    font-weight: 500;\n  }\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}